{
	"@type":"NativeView",
	"Name":"Top 10 Release Categories by Revenue",
	"Columns":
	[
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Sales Measure')/Hierarchies('Sales Measure')"
				},
				"Elements":
				[
					{
						"@id":"Dimensions('Sales Measure')/Hierarchies('Sales Measure')/Elements('Revenue')"
					}
				]
			}
		}
	],
	"Rows":
	[
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Release')/Hierarchies('Release')"
				},
				"Expression":"TOPCOUNT(TM1SubsetToSet([Release].[Release], \"Default\") , 10 , ([Sales Measure].[Sales Measure].[Revenue],[Version].[Version].[Actual],[Year].[Year].[2019],[Store].[Store].[All Stores],[Month].[Month].[All Months]))"
			}
		}
	],
	"Titles":
	[
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Version')/Hierarchies('Version')"
				},
				"Expression":"TM1SUBSETALL([Version])"
			},
			"Selected":
			{
				"@id":"Dimensions('Version')/Hierarchies('Version')/Elements('Actual')"
			}
		},
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Year')/Hierarchies('Year')"
				},
				"Expression":"TM1SUBSETALL([Year])"
			},
			"Selected":
			{
				"@id":"Dimensions('Year')/Hierarchies('Year')/Elements('2019')"
			}
		},
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Store')/Hierarchies('Store')"
				},
				"Expression":"TM1SUBSETALL([Store])"
			},
			"Selected":
			{
				"@id":"Dimensions('Store')/Hierarchies('Store')/Elements('All Stores')"
			}
		},
		{
			"Subset":
			{
				"Hierarchy":
				{
					"@id":"Dimensions('Month')/Hierarchies('Month')"
				},
				"Expression":"TM1SUBSETALL([Month])"
			},
			"Selected":
			{
				"@id":"Dimensions('Month')/Hierarchies('Month')/Elements('All Months')"
			}
		}
	],
	"SuppressEmptyColumns":false,
	"SuppressEmptyRows":false,
	"FormatString":"0.00"
}